#!/bin/sh
UUID=$1

cat  << EOF

insmod nativedisk
insmod usbms
insmod ehci
insmod usb
insmod ahci
insmod part_gpt
insmod part_msdos
insmod fat
search.uuid ${UUID} root
if [ -e /grub/grub.cfg ]; then
	set prefix=($root)/grub
	configfile /grub/grub.cfg
else
	echo "Could not find an example configuration file!"
fi

EOF
